{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "three-column-dashboard",
  "type": "registry:block",
  "title": "Three Column Dashboard",
  "description": "A responsive dashboard layout with three main columns using the Column component",
  "registryDependencies": [
    "column",
    "card",
    "button",
    "badge",
    "progress",
    "https://eco-design-system-2.vercel.app/r/components/brand-sidebar.json",
    "https://eco-design-system-2.vercel.app/r/themes/theme.json"
  ],
  "files": [
    {
      "path": "src/layouts/shell-layout.tsx",
      "content": "\"use client\";\n\nimport { Inter, Geist_Mono, Montserrat } from \"next/font/google\";\nimport React, { type ReactNode } from \"react\";\n\nimport { BrandHeader } from \"@/components/brand-header\";\nimport { BrandSidebar } from \"@/components/brand-sidebar\";\nimport { SidebarProvider } from \"@/components/ui/sidebar\";\nimport { Toaster } from \"@/components/ui/sonner\";\nimport { cn } from \"@/lib/utils\";\nimport { useIsMobile } from \"@/hooks/use-mobile\";\n\nimport \"@/app/globals.css\";\n\nconst InterSans = Inter({\n  subsets: [\"latin\"],\n  variable: \"--font-sans\",\n});\n\nconst GeistMono = Geist_Mono({\n  subsets: [\"latin\"],\n  variable: \"--font-mono\",\n});\n\nconst MontserratSerif = Montserrat({\n  subsets: [\"latin\"],\n  variable: \"--font-serif\",\n});\n\nexport default function ShellLayout({\n  children,\n}: Readonly<{\n  children: ReactNode;\n}>) {\n  const isMobile = useIsMobile();\n\n  return (\n    <html\n      lang=\"en\"\n      className={cn(\n        InterSans.variable,\n        GeistMono.variable,\n        MontserratSerif.variable,\n        \"bg-background text-foreground\",\n      )}\n    >\n      <body>\n        <SidebarProvider>\n          {isMobile && <BrandHeader />}\n          <BrandSidebar />\n          <main className=\"flex w-full justify-center\">\n            <div className=\"container\">{children}</div>\n          </main>\n          <Toaster />\n        </SidebarProvider>\n      </body>\n    </html>\n  );\n}\n",
      "type": "registry:file",
      "target": "app/layout.tsx"
    },
    {
      "path": "src/app/demo/[name]/blocks/three-column-dashboard-page.tsx",
      "content": "import { Column } from \"@/components/ui/column\";\nimport { Card, CardContent, CardDescription, CardHeader, CardTitle } from \"@/components/ui/card\";\nimport { Badge } from \"@/components/ui/badge\";\nimport { Button } from \"@/components/ui/button\";\nimport { Progress } from \"@/components/ui/progress\";\n\nexport default function ThreeColumnDashboardPage() {\n  return (\n    <div className=\"space-y-6\">\n      {/* Header */}\n      <div className=\"space-y-2\">\n        <h1 className=\"text-2xl font-bold\">Dashboard Overview</h1>\n        <p className=\"text-muted-foreground\">\n          Monitor your key metrics and performance indicators\n        </p>\n      </div>\n\n      {/* Three Column Layout */}\n      <div className=\"flex gap-6\">\n        {/* Left Column - Analytics */}\n        <Column\n          size=\"4\"\n          responsive={{ md: \"12\", lg: \"4\" }}\n          header={\n            <div className=\"flex items-center justify-between p-4 bg-card border rounded-t-lg\">\n              <h2 className=\"text-lg font-semibold\">Analytics</h2>\n              <Badge variant=\"default\">Live</Badge>\n            </div>\n          }\n          footer={\n            <div className=\"p-4 bg-card border rounded-b-lg\">\n              <Button variant=\"outline\" size=\"sm\" className=\"w-full\">\n                View Full Report\n              </Button>\n            </div>\n          }\n        >\n          <div className=\"p-4 bg-card border-x\">\n            <div className=\"space-y-4\">\n              <div className=\"space-y-2\">\n                <div className=\"flex justify-between text-sm\">\n                  <span className=\"text-muted-foreground\">Total Users</span>\n                  <span className=\"font-medium\">12,345</span>\n                </div>\n                <Progress value={75} className=\"h-2\" />\n              </div>\n\n              <div className=\"space-y-2\">\n                <div className=\"flex justify-between text-sm\">\n                  <span className=\"text-muted-foreground\">Revenue</span>\n                  <span className=\"font-medium\">$45,678</span>\n                </div>\n                <Progress value={60} className=\"h-2\" />\n              </div>\n\n              <div className=\"space-y-2\">\n                <div className=\"flex justify-between text-sm\">\n                  <span className=\"text-muted-foreground\">Conversion</span>\n                  <span className=\"font-medium\">3.2%</span>\n                </div>\n                <Progress value={45} className=\"h-2\" />\n              </div>\n            </div>\n          </div>\n        </Column>\n\n        {/* Center Column - Recent Activity */}\n        <Column\n          size=\"4\"\n          responsive={{ md: \"12\", lg: \"4\" }}\n          header={\n            <div className=\"flex items-center justify-between p-4 bg-card border rounded-t-lg\">\n              <h2 className=\"text-lg font-semibold\">Recent Activity</h2>\n              <Button variant=\"ghost\" size=\"sm\">Filter</Button>\n            </div>\n          }\n          footer={\n            <div className=\"p-4 bg-card border rounded-b-lg\">\n              <Button variant=\"outline\" size=\"sm\" className=\"w-full\">\n                View All Activity\n              </Button>\n            </div>\n          }\n        >\n          <div className=\"p-4 bg-card border-x\">\n            <div className=\"space-y-3\">\n              <div className=\"flex items-center space-x-3\">\n                <div className=\"w-2 h-2 bg-green-500 rounded-full\"></div>\n                <div className=\"flex-1 min-w-0\">\n                  <p className=\"text-sm font-medium\">New user registered</p>\n                  <p className=\"text-xs text-muted-foreground\">2 minutes ago</p>\n                </div>\n              </div>\n\n              <div className=\"flex items-center space-x-3\">\n                <div className=\"w-2 h-2 bg-blue-500 rounded-full\"></div>\n                <div className=\"flex-1 min-w-0\">\n                  <p className=\"text-sm font-medium\">Order completed</p>\n                  <p className=\"text-xs text-muted-foreground\">5 minutes ago</p>\n                </div>\n              </div>\n\n              <div className=\"flex items-center space-x-3\">\n                <div className=\"w-2 h-2 bg-yellow-500 rounded-full\"></div>\n                <div className=\"flex-1 min-w-0\">\n                  <p className=\"text-sm font-medium\">Payment pending</p>\n                  <p className=\"text-xs text-muted-foreground\">10 minutes ago</p>\n                </div>\n              </div>\n\n              <div className=\"flex items-center space-x-3\">\n                <div className=\"w-2 h-2 bg-red-500 rounded-full\"></div>\n                <div className=\"flex-1 min-w-0\">\n                  <p className=\"text-sm font-medium\">System alert</p>\n                  <p className=\"text-xs text-muted-foreground\">15 minutes ago</p>\n                </div>\n              </div>\n            </div>\n          </div>\n        </Column>\n\n        {/* Right Column - Quick Actions */}\n        <Column\n          size=\"4\"\n          responsive={{ md: \"12\", lg: \"4\" }}\n          header={\n            <div className=\"flex items-center justify-between p-4 bg-card border rounded-t-lg\">\n              <h2 className=\"text-lg font-semibold\">Quick Actions</h2>\n              <Badge variant=\"default\">Beta</Badge>\n            </div>\n          }\n          footer={\n            <div className=\"p-4 bg-card border rounded-b-lg\">\n              <Button size=\"sm\" className=\"w-full\">\n                Create New\n              </Button>\n            </div>\n          }\n        >\n          <div className=\"p-4 bg-card border-x\">\n            <div className=\"space-y-3\">\n              <Button variant=\"outline\" size=\"sm\" className=\"w-full justify-start\">\n                <span className=\"mr-2\">üìä</span>\n                Generate Report\n              </Button>\n\n              <Button variant=\"outline\" size=\"sm\" className=\"w-full justify-start\">\n                <span className=\"mr-2\">üìß</span>\n                Send Notification\n              </Button>\n\n              <Button variant=\"outline\" size=\"sm\" className=\"w-full justify-start\">\n                <span className=\"mr-2\">‚öôÔ∏è</span>\n                Settings\n              </Button>\n\n              <Button variant=\"outline\" size=\"sm\" className=\"w-full justify-start\">\n                <span className=\"mr-2\">üìà</span>\n                View Analytics\n              </Button>\n            </div>\n          </div>\n        </Column>\n      </div>\n\n      {/* Additional Content Row */}\n      <div className=\"flex gap-6\">\n        <Column\n          size=\"6\"\n          responsive={{ md: \"12\", lg: \"6\" }}\n          header={\n            <div className=\"p-4 bg-card border rounded-t-lg\">\n              <h2 className=\"text-lg font-semibold\">Performance Metrics</h2>\n            </div>\n          }\n        >\n          <div className=\"p-4 bg-card border-x border-b rounded-b-lg\">\n            <div className=\"grid grid-cols-2 gap-4\">\n              <Card>\n                <CardHeader className=\"pb-2\">\n                  <CardTitle className=\"text-sm\">Page Views</CardTitle>\n                </CardHeader>\n                <CardContent>\n                  <div className=\"text-2xl font-bold\">45,231</div>\n                  <p className=\"text-xs text-muted-foreground\">+20.1% from last month</p>\n                </CardContent>\n              </Card>\n\n              <Card>\n                <CardHeader className=\"pb-2\">\n                  <CardTitle className=\"text-sm\">Bounce Rate</CardTitle>\n                </CardHeader>\n                <CardContent>\n                  <div className=\"text-2xl font-bold\">32.5%</div>\n                  <p className=\"text-xs text-muted-foreground\">-5.2% from last month</p>\n                </CardContent>\n              </Card>\n            </div>\n          </div>\n        </Column>\n\n        <Column\n          size=\"4\"\n          responsive={{ md: \"12\", lg: \"4\" }}\n          header={\n            <div className=\"p-4 bg-card border rounded-t-lg\">\n              <h2 className=\"text-lg font-semibold\">System Status</h2>\n            </div>\n          }\n        >\n          <div className=\"p-4 bg-card border-x border-b rounded-b-lg\">\n            <div className=\"space-y-3\">\n              <div className=\"flex items-center justify-between\">\n                <span className=\"text-sm\">API Status</span>\n                <Badge variant=\"default\" className=\"bg-green-500\">Online</Badge>\n              </div>\n\n              <div className=\"flex items-center justify-between\">\n                <span className=\"text-sm\">Database</span>\n                <Badge variant=\"default\" className=\"bg-green-500\">Healthy</Badge>\n              </div>\n\n              <div className=\"flex items-center justify-between\">\n                <span className=\"text-sm\">CDN</span>\n                <Badge variant=\"default\" className=\"bg-green-500\">Active</Badge>\n              </div>\n\n              <div className=\"flex items-center justify-between\">\n                <span className=\"text-sm\">Monitoring</span>\n                <Badge variant=\"warning\">Warning</Badge>\n              </div>\n            </div>\n          </div>\n        </Column>\n      </div>\n    </div>\n  );\n}\n",
      "type": "registry:page",
      "target": "app/page.tsx"
    }
  ]
}
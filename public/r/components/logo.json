      "content": "import { cn } from \"@/lib/utils\";\n\ninterface LogoProps {\n  collapsed?: boolean;\n  inSidebar?: boolean;\n}\n\nexport function Logo({ collapsed = false, inSidebar = false }: LogoProps) {\n  return (\n    <div className=\"flex items-center gap-2\">\n        <svg\n          width=\"23\"\n          height=\"22\"\n          viewBox=\"0 0 23 22\"\n          fill=\"none\"\n          xmlns=\"http://www.w3.org/2000/svg\"\n          className=\"flex-shrink-0\"\n        >\n        <path d=\"M11.5266 15.5687C10.2941 15.5586 9.11621 15.0688 8.25187 14.2069C7.38753 13.3451 6.90759 12.1817 6.91754 10.9728H0.527987C0.499569 12.4072 0.762958 13.8327 1.30289 15.166C1.84283 16.4993 2.64841 17.7136 3.67248 18.7379C4.69655 19.7621 5.91858 20.5758 7.26705 21.1312C8.61552 21.6867 10.0634 21.9728 11.5259 21.9728C12.9884 21.9728 14.4363 21.6867 15.7848 21.1312C17.1332 20.5758 18.3552 19.7621 19.3792 18.7379C20.4033 17.7136 21.2089 16.4993 21.7488 15.166C22.2888 13.8327 22.5522 12.4072 22.5238 10.9728H16.1315C16.1414 12.1817 15.6615 13.3451 14.7972 14.2069C13.9328 15.0688 12.7549 15.5586 11.5224 15.5687\" fill=\"#68C949\"/>\n        <path d=\"M11.5276 0C8.58665 0.0237508 5.77574 1.19599 3.71319 3.25887C1.65063 5.32175 0.505392 8.10631 0.52936 11H6.91949C6.91949 9.79713 7.40512 8.64353 8.26957 7.79297C9.13402 6.94242 10.3064 6.46457 11.5289 6.46457C12.7515 6.46457 13.924 6.94242 14.7884 7.79297C15.6529 8.64353 16.1385 9.79713 16.1385 11H22.5286C22.5405 9.56708 22.2654 8.14588 21.719 6.81758C21.1726 5.48927 20.3657 4.27985 19.3443 3.25839C18.3229 2.23694 17.107 1.42347 15.766 0.864387C14.4251 0.305308 12.9839 0.0115822 11.5276 0Z\" fill=\"#1497E3\"/>\n        </svg>\n\n      <span className={cn(\n        \"font-regular text-sm whitespace-nowrap transition-all duration-300 ease-in-out\",\n        inSidebar ? \"text-sidebar-foreground\" : \"text-foreground\",\n        collapsed ? \"opacity-0 max-w-0 overflow-hidden\" : \"opacity-100 max-w-xs\"\n      )}>Product Name</span>\n    </div>\n  );\n}\n",
